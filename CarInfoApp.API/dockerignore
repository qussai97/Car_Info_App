*.userosscache
*.sln.docstates

# Ignore Visual Studio temporary files
.vs/

# Ignore NuGet packages
*.nupkg
packages/

# Ignore logs
*.log

# Ignore Dockerfile itself and docker-compose files if you want
Dockerfile
docker-compose*

# Ignore publish output if you are publishing separately
publish/

# Ignore environment files if any
.env

# Ignore Rider IDE files if used
.idea/

# Ignore test results
TestResults/

# Ignore files that can contain secrets or local config
appsettings.Development.json
